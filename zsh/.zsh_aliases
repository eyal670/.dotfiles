
alias cpwd="pwd | tr -d '\n' | xclip -selection clipboard"
alias tmux="tmux -u"
alias lspt="sh ~/Scripts/launchspt.sh"
alias song_notify="sh ~/Scripts/songnotification.sh"
alias wttr="curl wttr.in/Atlit"
alias fcd='cd $(find $HOME/DevLab/ -type d 2>&1 | grep -v "Permission denied" | fzf)'
alias lcd='cd $(find $HOME/LocalWP/ -type d 2>&1 | grep -v "Permission denied" | fzf)'
alias zcd='cd $(zoxide query -l | fzf)'
alias tcd='sh ~/.scripts/t.sh'
alias cht='sh ~/.scripts/cheat.sh'
alias ntemp='nvim ~/DevLab/temp/' 
alias nvim_php='nvim ${PWD##*/}.php' 
alias timestamp="date +%s | xclip -selection clipboard && notify-send \"timestamp copy to clipbaord\""
alias zipf='zipfolder'
alias sp='spotify_player'
alias ssh='TERM=xterm-256color ssh'
alias clc='fc -nl -1 | xclip -selection clipboard && echo "command copied to clipboard: $(fc -nl -1)"'
alias ls='ls --color=auto'
